cx:Seller a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Sales> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:sells ;
            owl:someValuesFrom cx:Product ],
        cx:Role ;
    skos:altLabel "salesman"@en,
        "salesperson"@en,
        "saleswoman"@en ;
    skos:prefLabel "Verkäufer"@de,
        "Seller"@en .

cx:Service a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Service_>,
        schema:Service ;
    rdfs:subClassOf cx:ImmaterialThing,
        cx:Product ;
    owl:disjointWith cx:Goods ;
    skos:definition "intangible, not transferable product"@en ;
    skos:prefLabel "Dienstleistung"@de,
        "Service"@en .

cx:Customer a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:purchases ;
            owl:someValuesFrom cx:Product ],
        [ a owl:Restriction ;
            owl:onProperty cx:purchasesFrom ;
            owl:someValuesFrom cx:Seller ],
        cx:Person ;
    skos:prefLabel "Kunde"@de,
        "Customer"@en .

cx:EclipseDataspaceConnector a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso <https://projects.eclipse.org/proposals/eclipse-dataspace-connector> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:eclipseDataspaceConnectorId ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:eclipseDataspaceConnectorUrl ;
            owl:someValuesFrom xsd:string ],
        cx:ImmaterialThing ;
    skos:altLabel "EDC"@en,
        "eclipse dataspace connector"@en ;
    skos:prefLabel "Connector"@en .

cx:FinalCustomer a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:finalCustomerId ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:purchases ;
            owl:someValuesFrom cx:FinalProduct ],
        cx:Organisation,
        cx:Person ;
    skos:altLabel "Endnutzer"@de,
        "end user"@en ;
    skos:prefLabel "Endkunde"@de,
        "Final Customer"@en .

cx:Information a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:subClassOf cx:ImmaterialThing ;
    skos:prefLabel "Information"@en .

cx:Offer a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso schema:Offer ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:isOfferedBy ;
            owl:someValuesFrom cx:Organisation ],
        [ a owl:Restriction ;
            owl:onProperty cx:isOfferedBy ;
            owl:someValuesFrom cx:Person ],
        cx:Event ;
    skos:altLabel "offering"@en ;
    skos:prefLabel "Offer"@en .

cx:Plant a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:plantId ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:hasStreetAddress ;
            owl:someValuesFrom cx:StreetAddress ],
        [ a owl:Restriction ;
            owl:onProperty cx:plantName ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:produces ;
            owl:someValuesFrom cx:Product ],
        cx:Place ;
    skos:prefLabel "Plant"@en .

cx:sells a owl:ObjectProperty ;
    rdfs:domain cx:Reseller,
        cx:Retailer,
        cx:Seller,
        cx:Wholesaler ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:range cx:Product ;
    skos:prefLabel "sells"@en .

cx:produces a owl:ObjectProperty ;
    rdfs:domain cx:Assembly,
        cx:Manufacturer,
        cx:Manufacturing,
        cx:Plant,
        cx:Production ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:range cx:Product ;
    skos:prefLabel "produces"@en .

cx:BusinessPartner a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Business_partner> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:businessPartnerNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:businessPartnerNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:hasConnector ;
            owl:someValuesFrom cx:EclipseDataspaceConnector ],
        [ a owl:Restriction ;
            owl:onProperty cx:businessPartnerNumberType ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:hasRole ;
            owl:someValuesFrom cx:Role ],
        cx:Company ;
    skos:altLabel "BP"@en,
        "business partner"@en ;
    skos:definition "A Business Partner is a legal entity that is part of the Catena-X network or that stands in a relevant relation to a Catena-X Business Partner."@en ;
    skos:example "Audi"@en,
        "BMW"@en,
        "Mercedes"@en,
        "Volkswagen"@en ;
    skos:prefLabel "Geschäftspartner"@de,
        "Business Partner"@en .

cx:Manufacturer a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso schema:manufacturer ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:isSuppliedBy ;
            owl:someValuesFrom cx:Supplier ],
        [ a owl:Restriction ;
            owl:onProperty cx:produces ;
            owl:someValuesFrom cx:Product ],
        [ a owl:Restriction ;
            owl:onProperty cx:manufacturerName ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:manufacturerId ;
            owl:someValuesFrom xsd:string ],
        cx:Role ;
    skos:prefLabel "Hersteller"@de,
        "Manufacturer"@en .

cx:TechnicalStandard a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Technical_standard> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:technicalStandardStandardNumber ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:technicalStandardStandardName ;
            owl:someValuesFrom xsd:string ],
        cx:ImmaterialThing ;
    skos:prefLabel "Technical Standard"@en .

cx:Person rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:isMemberOf ;
            owl:someValuesFrom cx:Project ],
        [ a owl:Restriction ;
            owl:onProperty cx:hasContact ;
            owl:someValuesFrom cx:Contact ],
        [ a owl:Restriction ;
            owl:onProperty cx:makesOffer ;
            owl:someValuesFrom cx:Offer ] .

cx:Supplier a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Vendor> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:suppliesTo ;
            owl:someValuesFrom cx:Manufacturer ],
        [ a owl:Restriction ;
            owl:onProperty cx:supplierId ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:supplierName ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:supplies ;
            owl:someValuesFrom cx:Product ],
        cx:Role ;
    skos:altLabel "provider"@en,
        "seller"@en,
        "supplier"@en,
        "vendor"@en ;
    skos:prefLabel "Lieferant"@de,
        "Supplier"@en .

cx:Event rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:eventDate ;
            owl:someValuesFrom xsd:date ],
        [ a owl:Restriction ;
            owl:onProperty cx:actionId ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:actionName ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:eventName ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:eventType ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:eventLocation ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:eventId ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:eventDuration ;
            owl:someValuesFrom xsd:integer ] .

cx:Order a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso schema:Order ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:orderUnit ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:isOrderedFrom ;
            owl:someValuesFrom cx:Country ],
        [ a owl:Restriction ;
            owl:onProperty cx:isDeliveredBy ;
            owl:someValuesFrom cx:Manufacturer ],
        [ a owl:Restriction ;
            owl:onProperty cx:isDeliveredBy ;
            owl:someValuesFrom cx:Supplier ],
        [ a owl:Restriction ;
            owl:onProperty cx:isOrderedBy ;
            owl:someValuesFrom cx:FinalCustomer ],
        [ a owl:Restriction ;
            owl:onProperty cx:orderCost ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty cx:orderQuantity ;
            owl:someValuesFrom xsd:decimal ],
        [ a owl:Restriction ;
            owl:onProperty cx:orderCurrency ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:orderId ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:isOrderedBy ;
            owl:someValuesFrom cx:Customer ],
        [ a owl:Restriction ;
            owl:onProperty cx:orderDatetime ;
            owl:someValuesFrom xsd:dateTime ],
        [ a owl:Restriction ;
            owl:onProperty cx:orderPrice ;
            owl:someValuesFrom xsd:decimal ],
        cx:Event ;
    skos:prefLabel "Order"@en .

cx:Organisation rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:makesOffer ;
            owl:someValuesFrom cx:Offer ],
        [ a owl:Restriction ;
            owl:onProperty cx:isMemberOf ;
            owl:someValuesFrom cx:Consortium ] .

cx:Role a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> ;
    rdfs:seeAlso schema:Role ;
    rdfs:subClassOf cx:ImmaterialThing ;
    skos:prefLabel "Rolle"@de,
        "Role"@en .

cx:Product rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:cost ;
            owl:someValuesFrom xsd:float ],
        [ a owl:Restriction ;
            owl:onProperty cx:price ;
            owl:someValuesFrom xsd:float ],
        [ a owl:Restriction ;
            owl:onProperty cx:hasBrand ;
            owl:someValuesFrom cx:Brand ] .

<https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/common_ontology.ttl> a owl:Ontology ;
    dc:creator "Zazralt Magic" ;
    dc:date "2022-12-02"^^xsd:date ;
    dc:description "The Catena-X common ontology contains all domain-independent classes, attributes and relations." ;
    dc:title "Common Ontology" ;
    owl:imports <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/upper_ontology.ttl> ;
    owl:versionInfo "0.0.1" .

