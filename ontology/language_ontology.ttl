cx:Language a owl:Class ;
    rdfs:domain cx:Language ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/language_ontology.ttl> ;
    rdfs:range xsd:string ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Language> ;
    rdfs:subClassOf [ a owl:Restriction ;
