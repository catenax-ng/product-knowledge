cx-diag: rdf:type owl:Ontology ;
    rdfs:comment "The Catena-X Diagnosis Domain describes knowledge about automotive (electronic) diagnosis processes."@en ;
    rdfs:label "Catena-X Diagnosis Domain" ;
    owl:versionInfo "0.5.1" .

cx-diag:DTC rdf:type owl:Class ;
    rdfs:comment "A Diagnostic Trouble Code (DTC) is a (BusinessPartner-specific) trouble condition that is (either directly or in parts) indicated through an electronic diagnosis."@en ;
    rdfs:label "Diagnostic Trouble Code" ;
    rdfs:subClassOf cx:Asset .

cx-diag:Code rdf:type owl:DatatypeProperty ;
    rdfs:comment "The Code of a Diagnostic Trouble Code (DTC) is a (BusinessPartner-specific) identifier that is shown in protocols."@en ;
    rdfs:label "DTC Code" ;
    rdfs:domain cx-diag:DTC ;
    rdfs:range xsd:string .

cx-diag:Description rdf:type owl:DatatypeProperty ;
    rdfs:comment "The Description of a Diagnostic Trouble Code (DTC) is an interationalized paragraph/sentence describing a trouble condition."@en ;
    rdfs:comment "Deutsche Beschreibung."@de ;
    rdfs:label "DTC Description" ;
    rdfs:domain cx-diag:DTC ;
    rdfs:range xsd:string .

cx-diag:PossibleCauses rdf:type owl:DatatypeProperty ;
    rdfs:comment "The Possible Causes of a Diagnostic Trouble Code (DTC) are identifiers to possible root causes of a trouble condition."@en ;
    rdfs:label "DTC Possible Causes" ;
    rdfs:domain cx-diag:DTC ;
    rdfs:range xsd:string .

cx-diag:Version rdf:type owl:DatatypeProperty ;
    rdfs:comment "The Version of a Diagnostic Trouble Code (DTC) describes the version of the condition in the Business Partners DTC catalogue."@en ;
    rdfs:label "DTC Version" ;
    rdfs:domain cx-diag:DTC ;
    rdfs:range xsd:long .

cx-diag:DiagnosedPart rdf:type owl:Class ;
    rdfs:comment "A Diagnosed Part describes any aggregate/component that is observed to an electronic diagnosis."@en ;
    rdfs:label "Diagnosed Part" ;
    rdfs:subClassOf cx:Asset .

cx-diag:EnDenomination rdf:type owl:DatatypeProperty ;
    rdfs:comment "The EnDenomination of a Diagnosed Part is an internationalized unique name."@en ;
    rdfs:label "Diagnosed Part EnDenomination" ;
    rdfs:domain cx-diag:DiagnosedPart ;
    rdfs:range xsd:string .

cx-diag:Classification rdf:type owl:DatatypeProperty ;
    rdfs:comment "The Classification of a Diagnosed Part is what?."@en ;
    rdfs:label "Diagnosed Part Classification" ;
    rdfs:domain cx-diag:DiagnosedPart ;
    rdfs:range xsd:string .

cx-diag:Category rdf:type owl:DatatypeProperty ;
    rdfs:comment "The Category of a Diagnosed Part categorizes the part in a hierarchy."@en ;
    rdfs:label "Diagnosed Part Category" ;
    rdfs:domain cx-diag:DiagnosedPart ;
    rdfs:range xsd:string .

cx-diag:affects rdf:type owl:ObjectProperty ;
    rdfs:comment "The affects predicate connects Diagnostic Trouble Codes (DTC) with the diagnosed parts."@en ;
    rdfs:label "Affects" ;
    rdfs:domain cx-diag:DTC ;
    rdfs:range cx-diag:DiagnosedPart .

