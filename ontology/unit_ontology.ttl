    skos:prefLabel "Lux"@en .

cx:SiDerivedUnit_Newton a cx:SiDerivedUnit ;
    skos:altLabel "N"@en ;
    skos:note "force, weight"@en ;
    skos:prefLabel "Newton"@en .

