cx:InternationalSystem a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/unit_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/International_System_of_Units> ;
    rdfs:subClassOf cx:SystemOfMeasurement ;
    skos:altLabel "SI System"@de,
        "international system of units"@en,
        "si system"@en ;
    skos:prefLabel "International System"@en .

cx:SiBaseUnit_Ampere a cx:SiBaseUnit ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Ampere> ;
    skos:altLabel "A"@en ;
    skos:note "electric current"@en ;
    skos:prefLabel "Ampere"@en .

