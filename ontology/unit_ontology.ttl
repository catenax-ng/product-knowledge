cx:SystemOfMeasurement a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/unit_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/System_of_measurement> ;
    rdfs:subClassOf cx:ImmaterialThing ;
    skos:prefLabel "Einheitensystem"@de,
        "System Of Measurement"@en .

cx:unitId a owl:DatatypeProperty ;
    rdfs:domain cx:Unit ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/unit_ontology.ttl> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf cx:identifier ;
    skos:prefLabel "Unit Id"@en .

cx:unitName a owl:DatatypeProperty ;
    rdfs:domain cx:Unit ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/unit_ontology.ttl> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf cx:name ;
    skos:prefLabel "Unit Name"@en .

cx:Unit a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/unit_ontology.ttl> ;
    rdfs:seeAlso schema:unitCode,
        schema:unitText ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:unitName ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:unitId ;
            owl:someValuesFrom xsd:string ],
        cx:ImmaterialThing ;
    skos:prefLabel "Unit"@en .

cx:SiBaseUnit a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/unit_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/International_System_of_Units#SI_base_units> ;
    rdfs:subClassOf cx:Unit ;
    skos:prefLabel "Si Base Unit"@en .

<https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/unit_ontology.ttl> a owl:Ontology ;
    dc:creator "Zazralt Magic" ;
    dc:date "2023-02-21"^^xsd:date ;
    dc:title "Unit Ontology" ;
    owl:versionInfo "0.0.1" .

cx:SiDerivedUnit a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/unit_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/International_System_of_Units#SI_derived_units> ;
    rdfs:subClassOf cx:Unit ;
    skos:prefLabel "Si Derived Unit"@en .

