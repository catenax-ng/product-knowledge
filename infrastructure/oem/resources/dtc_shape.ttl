@prefix : <urn:cx:graph:oem:Diagnosis2022> .
@prefix cx: <https://github.com/catenax-ng/product-knowledge/ontology/cx.ttl#> .
@prefix cx-diag: <https://github.com/catenax-ng/product-knowledge/ontology/diagnosis.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

OemDTC rdf:type sh:NodeShape ;
  sh:targetClass cx-diag:DTC ;
  sh:property [
        sh:path cx:provisionedBy ;
        sh:hasValue <https://github.com/catenax-ng/product-knowledge/ontology/cx.ttl#BusinessPartner/BPNL00000003COJN> ;
    ] ;
  sh:property [
        sh:path cx-diag:Version ;
        sh:hasValue 0^^xsd:long ;
    ] ;
  sh:property [
        sh:path cx-diag:Affects ;
        sh:class OemDiagnosedParts ;
    ] ;

OemDiagnosedParts rdf:type sh:NodeShape ;
  sh:targetClass cx-diag:DiagnosedPart ;
  sh:property [
        sh:path cx:provisionedBy ;
        sh:hasValue <https://github.com/catenax-ng/product-knowledge/ontology/cx.ttl#BusinessPartner/BPNL00000003COJN> ;
    ] ;
